<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\version.props" />

  <PropertyGroup>
    <Description>Attribute to mark multiple startup classes, being able to load information from them</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>Swashbuckle.AspNetCore.SwaggerStartupAttr</AssemblyName>
    <AssemblyOriginatorKeyFile>Swashbuckle.AspNetCore.SwaggerStartupAttr.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PackageId>Swashbuckle.AspNetCore.SwaggerStartupAttr</PackageId>
    <PackageTags>swagger;documentation;discovery;help;webapi;aspnet;aspnetcore;attribute;startup</PackageTags>
    <PackageProjectUrl>https://github.com/itsprdsw/Swashbuckle.AspNetCore</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/itsprdsw/Swashbuckle.AspNetCore/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/itsprdsw/Swashbuckle.AspNetCore.git</RepositoryUrl>
  </PropertyGroup>

</Project>
